{
	"build": {
		"beforeDevCommand": "",
		"beforeBuildCommand": "",
		"devPath": "../src",
		"distDir": "../src",
		"withGlobalTauri": true
	},
	"package": {
		"productName": "id3-tag-editor",
		"version": "0.0.0"
	},
	"tauri": {
		"allowlist": {
			"all": true,
			"dialog": {
				"all": true,
				"ask": true,
				"confirm": true,
				"message": true,
				"open": true,
				"save": true
			},
			"shell": {
				"all": false,
				"open": true
			}
		},
		"bundle": {
			"active": true,
			"targets": "all",
			"identifier": "com.id3-tag-editor.dev",
			"icon": [
				"icons/32x32.png",
				"icons/128x128.png",
				"icons/128x128@2x.png",
				"icons/icon.icns",
				"icons/icon.ico"
			]
		},
		"security": {
			"csp": null
		},
		"windows": [
			{
				"fullscreen": false,
				"resizable": true,
				"title": "id3-tag-editor",
				"width": 800,
				"height": 600
			}
		]
	}
}